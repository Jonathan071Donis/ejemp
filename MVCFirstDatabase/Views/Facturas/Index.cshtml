@model IEnumerable<MVCFirstDatabase.Models.Factura>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nit)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vehiculo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Total)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descuento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkClienteNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoFacturaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nit)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vehiculo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Total)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descuento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkClienteNavigation.NoIdentificacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoFacturaNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NoFactura">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.NoFactura">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.NoFactura">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
